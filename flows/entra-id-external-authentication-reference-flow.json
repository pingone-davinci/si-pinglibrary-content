              },
              "code": {
                "value": "const findFirstKeyWithValue = (map, values) => {\n  for (const value of values) {\n    const lowerCaseValue = value.toLowerCase();\n    for (const [key, array] of Object.entries(map)) {\n      if (array.some(item => item.toLowerCase() === lowerCaseValue)) {\n        return key.toLowerCase();\n      }\n    }\n  }\n  return null;\n};\n\nconst entraIdAmrToPingIdAmrMap = {\n  FACE: [],\n  FIDO: [\"fidokey\", \"fidoplat\", \"fido\"],\n  FPT: [],\n  HWK: [\"yupiotp\", \"oath\"],\n  IRIS: [],\n  OTP: [\"mobileotp\", \"pidotp\", \"email\", \"desktop\", \"authapp\"],\n  POP: [],\n  RETINA: [],\n  SC: [],\n  SMS: [\"sms\"],\n  SWK: [\"swk\", \"pidpush\", \"pidnm\"],\n  TEL: [\"tel\"],\n  VBM: [],\n  BIO: [\"pidfpt\", \"pidnm\"]\n};\n\nmodule.exports = a = async ({params}) => {\n  const pingIdError = params.error;\n\tconst pingIdAmrOutput = params.pingIdAmr;\n  const pingIdAmrArray = pingIdAmrOutput.split(' ');\n\n\tlet entraIdAmr = findFirstKeyWithValue(entraIdAmrToPingIdAmrMap, pingIdAmrArray);\n  if (entraIdAmr == null && !pingIdError) {\n    entraIdAmr = \"pop\";\n  }\n\n\treturn { entraIdAmr };\n};"
              },
              "outputSchema": {
                "value": "{\n\t\"output\": {\n\t\t\"type\": \"object\",\n\t\t\"properties\": {\n\t\t\t\"entraIdAmr\": {\n\t\t\t\t\"type\": \"string\"\n\t\t\t}\n\t\t}\n\t}\n}"
              },
